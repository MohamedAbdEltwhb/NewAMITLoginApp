<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="NoAppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="edetTextRegester">
        <item name="android:layout_width">320dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:background">@drawable/edittext</item>
    </style>

    <style name="NoAppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="NoAppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>
